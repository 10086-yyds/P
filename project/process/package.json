{
  "name": "process",
  "version": "1.0.0",
  "description": "工程小程序 - uni-app前端项目",
  "main": "main.js",
  "scripts": {
    "dev": "npm run dev:mp-weixin",
    "dev:mp-weixin": "uni build --watch --platform mp-weixin",
    "build": "npm run build:mp-weixin",
    "build:mp-weixin": "uni build --platform mp-weixin",
    "lint": "eslint --ext .js,.vue .",
    "lint:fix": "eslint --ext .js,.vue . --fix",
    "format": "prettier --write \"**/*.{js,vue,json,css,scss}\"",
    "format:check": "prettier --check \"**/*.{js,vue,json,css,scss}\""
  },
  "devDependencies": {
    "eslint": "^8.50.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-vue": "^9.17.0",
    "prettier": "^3.0.0"
  }
}